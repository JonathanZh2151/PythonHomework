cases = int(input()) # the number of songs
songs = list(map(int, input().split())) # the list of songs
unique = 0 # the greatest number of unique integers in a row
for start in range(cases - 1): #  the start of the slice
    for end in range(start + 1, cases + 1): # the end of the slice of list
        if (len(set(songs[start : end])) == end - start): # if the unique numbers in the list is equal to the length of the slice
            unique = max(unique, end - start) # unique equals the length of the list
if (cases == 1):
    unique = 1
print(unique) # print the number of maximum number of unique integers in a row

'''
#! SAMPLE CODE
Input:
8
1 2 1 3 2 7 4 2

Output:
5
'''
